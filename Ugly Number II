//https://leetcode.com/problems/ugly-number-ii/
class Solution {
public:
    int nthUglyNumber(int n) {
        set<size_t> s;
        s.insert(1);
        size_t res=0;
        for(int i = 0;i<n;i++){
            res=*s.lower_bound(res+1);
            s.insert(res*2);
            s.insert(res*3);
            s.insert(res*5);
        }
        return res;
    }
   
};
