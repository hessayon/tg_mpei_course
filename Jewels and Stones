//https://leetcode.com/problems/jewels-and-stones/
class Solution {
public:
    int numJewelsInStones(string J, string S) {
        int count=0;
        int nj=J.size();
        int ns=S.size();
        for(int i=0;i<nj;i++)
            for(int j=0;j<ns;j++)
                if (J[i]==S[j])
                    count++;
        return count;
    }
};
